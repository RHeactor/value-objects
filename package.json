{
  "name": "rheactor-value-objects",
  "description": "A collection of value objects",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/standard",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "environment=testing ./node_modules/.bin/mocha --reporter=dot --timeout=1350 test/\\*.js",
    "test:coverage": "environment=testing ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R dot --timeout=1350 test/\\*.js && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ResourcefulHumans/rheactor-value-objects.git"
  },
  "author": "Resourceful Humans <info@resourceful-humans.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ResourcefulHumans/rheactor-value-objects/issues"
  },
  "homepage": "https://github.com/ResourcefulHumans/rheactor-value-objects#readme",
  "dependencies": {
    "joi": "^9.0.4",
    "lodash": "^4.14.0",
    "tcomb": "^3.2.6"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codacy-coverage": "^2.0.0",
    "ghooks": "^1.3.2",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.0",
    "mocha-lcov-reporter": "^1.2.0",
    "standard": "^7.1.0",
    "validate-commit-msg": "^2.6.1",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "validate-commit-msg"
    }
  }
}
